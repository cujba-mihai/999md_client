{
  "name": "next-js-boilerplate",
  "version": "2.1.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next build",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next out",
    "lighthouse": "lhci autorun --config=./.github/workflows/lighthouserc.json",
    "serve:lhci": "npm start",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "update": "echo Updating dependencies... && npm i -g npm-check-updates && ncu -u && npm install",
    "volta": "curl https://get.volta.sh | bash && npm run install-node",
    "install-node": "volta install node"
  },
  "dependencies": {
    "cookies-next": "^2.1.1",
    "formik": "^2.2.9",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.2",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "mobile-detect": "^1.4.5",
    "next": "^13.0.5",
    "next-i18next": "^13.0.0",
    "next-images": "1.8.4",
    "next-seo": "5.15.0",
    "nuka-carousel": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "ua-parser-js": "^1.0.32",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next/bundle-analyzer": "13.1.1",
    "@testing-library/react": "13.4.0",
    "@types/i18next-sprintf-postprocessor": "^0.2.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "cssnano": "5.1.14",
    "eslint": "8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tailwindcss": "3.7.1",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "mini-css-extract-plugin": "^2.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "sass": "1.57.1",
    "start-server-and-test": "1.15.2",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "9.2.0"
  },
  "author": "Cujba Mihai, Daniel Raileanu"
}
