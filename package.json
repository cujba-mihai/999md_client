{
  "name": "next-js-boilerplate",
  "version": "2.1.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next build",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "prepare": "husky install",
    "update": "echo Updating dependencies... && npm i -g npm-check-updates && ncu -u && npm install",
    "volta": "curl https://get.volta.sh | bash && npm run install-node",
    "install-node": "volta install node"
  },
  "dependencies": {
    "next": "13.0.5",
    "next-images": "1.8.4",
    "next-seo": "5.14.1",
    "nuka-carousel": "5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next/bundle-analyzer": "13.0.5",
    "@testing-library/cypress": "8.0.7",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "cssnano": "5.1.14",
    "cypress": "11.2.0",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "13.0.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tailwindcss": "3.7.0",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "sass": "1.56.1",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "9.1.2"
  },
  "author": "Cujba Mihai, Daniel Raileanu"
}